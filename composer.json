{
    "name": "onlinetravelgroup/ean-client",
    "description": "Client for consuming the EAN Hotel API, based on Guzzle",
    "require": {
        "php": ">=5.4",
        "guzzlehttp/guzzle-services": "*"
    },
    "require-dev": {
        "guzzlehttp/log-subscriber": "*",
        "guzzlehttp/cache-subscriber": "*",
        "friendsofphp/php-cs-fixer": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "Otg\\Ean\\": "src/",
            "Otg\\Ean\\Tests\\": "test/"
        }
    },
    "authors": [
        {
            "name": "Ryan Hughes",
            "email": "rhughes@hotels.com.au"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "license": "MIT"
}
